include /nginx/upstreams-enabled/*.conf;

server {
    listen 80;

    include /nginx/http-locations-enabled/*.conf;
}

server {
    listen 443;

    ssl on;
    ssl_certificate     /nginx/ssl/app.crt;
    ssl_certificate_key /nginx/ssl/app.key;
    ssl_session_cache   shared:SSL:10m;

    include /nginx/https-locations-enabled/*.conf;
}
